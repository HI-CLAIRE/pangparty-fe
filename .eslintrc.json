{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
		"airbnb",
		"plugin:prettier/recommended",
		"plugin:react/jsx-runtime"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 14,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"prettier/prettier": ["error", { "endOfLine": "auto" }]
	}
}
